@model IEnumerable<SisArrendamiento.Models.LuzBaño>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AnteriorLuzLectura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ActualLuzLectura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KwConsumido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Monto)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AnteriorLuzLectura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ActualLuzLectura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KwConsumido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Monto)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Codigo">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Codigo">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Codigo">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
